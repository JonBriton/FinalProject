@page
@model IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Firearm[0].Brand)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Firearm[0].Model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Firearm[0].Caliber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Firearm[0].Type)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Firearm) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Brand)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Model)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Caliber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
        </tr>
}
    </tbody>
</table>
